{
  "name": "Calimero KNXnet/IP Server",
  "version": "0.15.3",
  "slug": "calimero_server",
  "description": "A KNXnet/IP server for running your own KNXnet/IP server in software accessing the KNX bus by KNXnet/IP, KNX IP, FT1.2, TPUART or USB (incl. RF) bus adapters",
  "arch": ["aarch64"],
  "url": "https://github.com/bohtho/hassio-addons/",
  "init": false,
  "startup": "services",
  "boot": "auto",
  "host_network": true,
  "uart": true,
  "usb": true,
  "options": {
    "knx_address": "0.1.0",
    "client_address_start": "0.1.101",
    "client_address_count": 5,
    "interface_type": "udp",
	"ip_address": "192.168.50.142",
	"routing": false,
    "knx_source_override": false
  },
  "schema": {
    "knx_address": "str",
    "client_address_start": "str",
    "client_address_count": "int",
    "interface_type": "list(udp|tcp|knxip|usb|ft12-cemi|tpuart)",
	"medium": "list(tp1|pl110|knxip|rf)?",
	"domain_address": "str?",
	"serial_device": "device(subsystem=tty)?",
	"ip_address": "str?",
	"usb_device": "str?",
	"routing": "bool",
    "knx_source_override": "bool?",
    "loglevel": "list(trace|debug|info|warn|error|off)?",
    "expose_date": "str?",
    "expose_time": "str?",
    "expose_datetime": "str?",
    "timeserver_expiration": "int?"
  }
}
